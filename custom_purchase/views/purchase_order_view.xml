<?xml version="1.0" encoding="utf-8"?>
<odoo>

		<record id="seq_purchase_order_inherit" model="ir.sequence">
            <field name="name">Purchase Order Sequence</field>
            <field name="code">pruchase.order.sequence</field>
            <field name="prefix">PO/</field>
            <field name="padding">3</field>
        </record>



      <record id="view_purchase_order_form_inherit12" model="ir.ui.view">
		<field name="name">purchase.order</field>
		<field name="model">purchase.order</field>
		<field name="inherit_id" ref="purchase.purchase_order_form"/>
		<field name="arch" type="xml">
			<xpath expr="//field[@name='date_approve']" position="after">
				<field name="purchase_reference" />
			</xpath>
		</field>
	</record> 
	 
 

 
</odoo>